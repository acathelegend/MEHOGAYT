local Tab2 = Window:MakeTab({
    Name = "Spam Skill",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section2 = Tab2:AddSection({
    Name = "Spamming Sword Skills"
})

local toggle = false 
local toggleCoroutine 

local function createToggle(skillName, waitTime, skill1, skill2)
    Section2:AddToggle({
        Name = skillName,
        Default = false,
        Callback = function(state)
            toggle = state
            if toggle then
                print("Toggle On")
                toggleCoroutine = coroutine.create(function()
                    while toggle do
                        wait(waitTime)
                        if skill2 then
                            game:GetService("Players").LocalPlayer.Character[skillName].Skill1.Fire:FireServer()
                            game:GetService("Players").LocalPlayer.Character[skillName].Skill2.Fire:FireServer()
                        else
                            game:GetService("Players").LocalPlayer.Character[skillName].Skill1.Fire:FireServer()
                        end
                    end
                end)
                coroutine.resume(toggleCoroutine)
            else
                print("Toggle Off")
                toggle = false
            end
        end    
    })
end

createToggle("FlashlightV1", 0.2, true)
createToggle("MushroomGODV1", 0.3, true)
createToggle("KatanaV2(LAG)", 0.3, true)
createToggle("KatanaV2(Fixed LAG)", 0.5, true)
createToggle("AlienSword", 0.3, true, true)
createToggle("Hammer", 0.1, false)
createToggle("KatanaV3", 0.2, false)
createToggle("DragonSword", 0.5, false)
createToggle("DiamondSword", 0.4, false)
createToggle("FloppawhiteSword", 0.5, false)

